public class LargestNumber {
    public static int largest(int[] arr){
        int max = arr[0];
        for(int i=0; i<arr.length; i++){
            if(arr[i]>max){
                max = arr[i];
            }
        }
        return max;
    }
    public static void main(String[] args) {
        int[] arr = {1,4,3,5,2};
        System.out.println("Largest array =" + largest(arr));
    }
}
